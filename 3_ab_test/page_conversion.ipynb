    "from statsmodels.stats.proportion import proportions_ztest\n",
    "from statsmodels.stats.power import NormalIndPower\n",
    "\n",
    "#parameters\n",
    "# p1 is the minimum detectable effect we want( 1% conversion increase)\n",
